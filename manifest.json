{
  "name": "Your Extension",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_security_policy": {
    "script-src": "self",
    "object-src": "self",
    "style-src": "self 'unsafe-inline'",
    "base-uri": "self"
  },
  "chrome_url_overrides" : {
    "newtab": "index.html"
  }
}
  